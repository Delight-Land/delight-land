<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.delightland.mapper.RadioMapper">

    <select id="findByno" resultType="String">
        select radio_name  
        from Radio  
        where radio_no = #{radio_no}
    </select>

</mapper>